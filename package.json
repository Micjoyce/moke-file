{
  "name": "moke-file",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start:docker": "cross-env NODE_ENV=producation EGG_SERVER_ENV=prod node --max-http-header-size=16384 server.js",
    "start": "cross-env PORT=7002 egg-scripts start --daemon --title=egg-server-moke-proxy",
    "stop": "egg-scripts stop --title=egg-server-moke-proxy",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts",
    "clean": "ets clean",
    "build": "npm run tsc && sh ./build.sh && npm run clean",
    "release": "standard-version && npm run build"
  },
  "dependencies": {
    "@hapi/boom": "^9.1.0",
    "@types/uuid": "^8.3.0",
    "ajv": "^6.12.3",
    "cross-env": "^7.0.2",
    "egg": "^2.27.0",
    "egg-healthy": "^1.0.0",
    "egg-mongoose": "^3.3.0",
    "egg-redis": "^2.4.0",
    "egg-router-plus": "^1.3.1",
    "egg-scripts": "^2.13.0",
    "egg-view-nunjucks": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "koa-useragent": "^4.0.0",
    "lodash": "^4.17.20",
    "minio": "^7.0.16",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa-ratelimit": "^4.2.1",
    "@types/lodash": "^4.14.159",
    "@types/minio": "^7.0.6",
    "@types/mocha": "^2.2.48",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^12.12.54",
    "@types/supertest": "^2.0.10",
    "autod": "^3.1.0",
    "autod-egg": "^1.1.0",
    "cz-conventional-changelog": "^3.2.0",
    "egg-bin": "^4.15.0",
    "egg-ci": "^1.15.0",
    "egg-mock": "^3.25.1",
    "eslint": "^6.8.0",
    "eslint-config-egg": "^8.0.1",
    "husky": "^4.2.5",
    "sneaky": "^2.0.1",
    "standard-version": "^7.1.0",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=12"
  },
  "ci": {
    "version": "12"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "",
  "license": "MIT"
}
